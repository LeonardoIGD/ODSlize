# Região AWS onde o Cognito está configurado
REACT_APP_AWS_REGION=us-east-1

# ID do User Pool do Cognito (obrigatório)
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx

# ID do Client da aplicação no User Pool (obrigatório)
REACT_APP_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# ID do Identity Pool (opcional - para acesso a recursos AWS)
REACT_APP_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Domínio do Hosted UI (opcional - se configurado)
REACT_APP_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com

# S3 Configuration (existente)
REACT_APP_S3_BUCKET_NAME=nome-do-bucket
REACT_APP_S3_REGION=us-east-1
